				"source": [
					"%%sql\r\n",
					"\r\n",
					"create or REPLACE TEMPORARY VIEW top_1000_ids\r\n",
					"AS\r\n",
					"    SELECT * from (\r\n",
					"        select *,\r\n",
					"            row_number() over(PARTITION by id order by id) as new_id\r\n",
					"            FROM top_ids\r\n",
					"        ) as a\r\n",
					"    where id > 1000\r\n",
					"    order by a.new_id asc"
				],

{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Moviesdata",
						"type": "DatasetReference"
					},
					"name": "Moviesdata"
				},
				{
					"name": "genredata"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Prop_0 as string,",
				"          id as string,",
				"          original_title as string,",
				"          overview as string,",
				"          genre_ids as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Moviesdata",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> genredata"
			]
		}
	}
}